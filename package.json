{
  "name": "memory-game",
  "version": "1.0.0",
  "description": "Memory game",
  "main": "main.js",
  "private": true,
  "author": "Sanne Karlsson",
  "homepage": "https://sannekarlsson.github.io/memory-game",
  "license": "MIT",
  "scripts": {
    "build": "parcel build index.html",
    "clear": "rm -rf dist",
    "deploy": "gh-pages -d dist",
    "lint": "eslint js/*.js || exit 0",
    "prebuild": "npm run prerun",
    "predeploy": "npm run build",
    "prerun": "npm run lint && npm run clear",
    "prestart": "npm run prerun",
    "start": "parcel index.html"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "gh-pages": "^2.0.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": true
    }
  }
}